{
  "name": "crypto-currency-clientapp",
  "description": "A crypto currency station client application.",
  "main": "index.js",
  "repository": "https://github.com/thangchung/modular-starter-kits",
  "author": "Thang Chung",
  "license": "MIT",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "cpx": "^1.5.0",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "react-scripts": "1.0.17"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-beta",
    "chart.js": "^2.7.0",
    "classnames": "^2.2.5",
    "flatpickr": "^4.1.4",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "humps": "^2.0.1",
    "jumpstate": "^2.2.2",
    "moment": "^2.18.1",
    "oidc-client": "^1.4.1",
    "oidc-client-fetch": "^1.2.2-fetch1",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-addons-css-transition-group": "^16.0.0-alpha.3",
    "react-addons-transition-group": "^16.0.0-alpha.3",
    "react-chartjs-2": "^2.6.4",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "react-select": "^1.1.0",
    "react-table": "^6.6.0",
    "react-transition-group": "^2.2.1",
    "reactstrap": "^5.0.0-alpha.3",
    "redux": "^3.7.2",
    "redux-form": "^7.1.2",
    "redux-observable": "^0.17.0",
    "redux-oidc": "^3.0.0-beta.14",
    "reselect": "^3.0.1",
    "rxjs": "^5.5.2",
    "screenfull": "^3.3.2",
    "simple-line-icons": "^2.4.1",
    "styled-components": "^2.2.4"
  },
  "scripts": {
    "copy-images": "cpx \"./src/**/*.{png,jpg}\" \"./public/server/\" --clean",
    "copy-styles": "cpx \"./src/**/*.css\" \"./public/server/\"",
    "copy-assets": "npm-run-all copy-styles copy-images",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "watch": "node ./scripts/watch.js",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css copy-assets start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all copy-assets build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:8888",
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 5.0.0"
  }
}
